{
  "document": {
    "block_prefix": "\n",
    "block_suffix": "\n",
    "color": "15",
    "margin": 2
  },
  "block_quote": {
    "indent": 1,
    "indent_token": "│ "
  },
  "paragraph": {},
  "list": {
    "level_indent": 2
  },
  "heading": {
    "block_suffix": "\n",
    "color": "12",
    "bold": true
  },
  "h1": {
    "prefix": " ",
    "suffix": " ",
    "color": "15",
    "background_color": "4",
    "bold": true,
    "underline": true
  },
  "h2": {
    "prefix": " ",
    "suffix": " ",
    "underline": true
  },
  "h3": {
    "prefix": "### "
  },
  "h4": {
    "prefix": "#### "
  },
  "h5": {
    "prefix": "##### "
  },
  "h6": {
    "prefix": "###### ",
    "color": "12",
    "bold": false
  },
  "text": {},
  "strikethrough": {
    "crossed_out": true
  },
  "emph": {
    "italic": true
  },
  "strong": {
    "bold": true
  },
  "hr": {
    "color": "8",
    "format": "\n--------\n"
  },
  "item": {
    "block_prefix": "* "
  },
  "enumeration": {
    "block_prefix": "- "
  },
  "task": {
    "ticked": "[x] ",
    "unticked": "[ ] "
  },
  "link": {
    "color": "2",
    "underline": true,
    "prefix": "<",
    "suffix": ">"
  },
  "link_text": {
    "color": "2",
    "bold": true,
    "prefix": "[",
    "suffix": "]"
  },
  "image": {
    "color": "5",
    "underline": true
  },
  "image_text": {
    "color": "13",
    "format": "Image: {{.text}} ->"
  },
  "code": {
    "prefix": " ",
    "suffix": " ",
    "color": "15",
    "background_color": "#252525",
    "bold": true
  },
  "code_block": {
    "color": "15",
    "background_color": "#252525",
    "margin": 2,
    "chroma": {
      "text":                  { "color": "#b9b9b9", "bold": false },
      "comment":               { "color": "#7c7c7c", "bold": false },
      "comment_preproc":       { "color": "#6d7144", "bold": false },
      "keyword":               { "color": "#903a3a", "bold": true },
      "keyword_reserved":      { "color": "#903a3a", "bold": true },
      "keyword_namespace":     { "color": "#903a3a", "bold": true },
      "keyword_type":          { "color": "#903a3a", "bold": true },
      "operator":              { "color": "#b9b9b9", "bold": false },
      "punctuation":           { "color": "#b9b9b9", "bold": false },
      "name":                  { "color": "#b9b9b9", "bold": false },
      "name_builtin":          { "color": "#903a3a", "bold": true },
      "name_tag":              { "color": "#61837e", "bold": true },
      "name_attribute":        { "color": "#61837e", "bold": true },
      "name_class":            { "color": "#903a3a", "bold": true },
      "name_constant":         { "color": "#903a3a", "bold": true },
      "name_decorator":        { "color": "#b9b9b9" },
      "name_exception":        {},
      "name_function":         { "color": "#5f819d" },
      "name_other":            { "color": "#b9b9b9" },
      "literal":               { "color": "#5f819d" },
      "literal_number":        { "color": "#5f819d", "bold": true },
      "literal_date":          { "color": "#5f819d", "bold": true },
      "literal_string":        { "color": "#9ca54e" },
      "literal_string_escape": { "color": "#af8431" },
      "generic_deleted":       { "color": "#903a3a", "bold": true },
      "generic_emph":          { "italic": true },
      "generic_inserted":      { "color": "#903a3a", "bold": true },
      "generic_strong":        { "bold": true },
      "generic_subheading":    { "color": "#903a3a", "bold": true },
      "background":            { "background_color": "#101010" }
    }
  },

  "table": {
    "center_separator": "┼",
    "column_separator": "│",
    "row_separator": "─"
  },
  "definition_list": {},
  "definition_term": {},
  "definition_description": {
    "block_prefix": "\n] "
  },
  "html_block": {},
  "html_span": {}
}
